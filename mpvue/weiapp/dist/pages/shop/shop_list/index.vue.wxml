<template name="6e4a28f5">
  <view class="_div data-v-774abbdc shop-list">
    <view class="_div data-v-774abbdc shop-list__item" wx:key="index" key="{{index}}" wx:for="{{shops}}" wx:for-index="index" wx:for-item="item">
      <view class="_div data-v-774abbdc shop-list__bd g-flex">
        <image class="_image data-v-774abbdc u-goods__img" src="{{item.img_url}}"></image>
        <view class="_div data-v-774abbdc g-flex__flex">
          <view class="_p data-v-774abbdc shop-list__name">{{item.name}}</view>
          <view class="_p data-v-774abbdc shop-list__address" wx:if="{{item.address}}"><label class="_span data-v-774abbdc iconfont icon-dingwei"></label>{{item.address}}</view>
          <view class="_p data-v-774abbdc shop-list__dist" wx:if="{{item.shop_code}}"><label class="_span data-v-774abbdc iconfont icon-daohang"></label></view>
        </view>
      </view>
      <view class="_div data-v-774abbdc shop-list__ft g-flex">
        <view class="_p data-v-774abbdc g-flex__item"><label class="_span data-v-774abbdc iconfont icon-phone"></label>电话</view>
        <view class="_p data-v-774abbdc g-flex__item"><label class="_span data-v-774abbdc iconfont icon-daohang"></label>导航</view>
      </view>
    </view>
  </view>
</template>